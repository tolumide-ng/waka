{
    "name": "grade-app",
    "version": "1.0.0",
    "description": "Frontend for grade app",
    "main": "index.js",
    "scripts": {
        "lint": "eslint --init",
        "lint:fix": "prettier-eslint \"$(pwd)/**/*.{js,json}\" --write",
        "prettier": "prettier \"$(pwd)/**/*.{js,json}\"  --write",
        "dev": "webpack-dev-server --hot --env.mode=development --open --port 5000",
        "build": "rm -rf ./dist && webpack --env.mode production",
        "heroku-postbuild": "npm run build",
        "start": "node server.js",
        "test": "jest --env=jsdom --coverage",
        "test:watch": "jest --env=jsdom --watch --coverage",
        "coverage": "cat ./coverage/lcov.info | coveralls"
    },
    "husky": {
        "hooks": {
            "pre-push": "lint-staged"
        }
    },
    "repository": {
